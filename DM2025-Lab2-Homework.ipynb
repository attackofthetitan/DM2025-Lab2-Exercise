    "class MSDClassifier(nn.Module):\n",
    "    def __init__(self, base_name: str, num_labels: int, n_drop: int = 5, p_drop: float = 0.5):\n",
    "        super().__init__()\n",
    "        self.backbone = AutoModel.from_pretrained(base_name)\n",
    "        hidden = self.backbone.config.hidden_size\n",
    "        self.dropouts = nn.ModuleList([nn.Dropout(p_drop) for _ in range(n_drop)])\n",
    "        self.classifier = nn.Linear(hidden, num_labels)\n",
    "\n",
    "        nn.init.normal_(self.classifier.weight, std=0.02)\n",
    "        nn.init.zeros_(self.classifier.bias)\n",
    "\n",
    "    @staticmethod\n",
    "    def mean_pool(last_hidden_state, attention_mask):\n",
    "        mask = attention_mask.unsqueeze(-1).to(last_hidden_state.dtype)  # [B, T, 1]\n",
    "        summed = (last_hidden_state * mask).sum(dim=1)                    # [B, H]\n",
    "        counts = mask.sum(dim=1).clamp(min=1e-6)                          # [B, 1]\n",
    "        return summed / counts\n",
    "\n",
    "    def forward(self, input_ids=None, attention_mask=None, labels=None, **kwargs):\n",
    "        kwargs.pop(\"num_items_in_batch\", None)\n",
    "        out = self.backbone(input_ids=input_ids, attention_mask=attention_mask, **kwargs)\n",
    "        pooled = self.mean_pool(out.last_hidden_state, attention_mask)\n",
    "        logits = torch.stack([self.classifier(dp(pooled)) for dp in self.dropouts], dim=0).mean(0)\n",
    "\n",
    "        loss = None\n",
    "        if labels is not None:\n",
    "            loss = F.cross_entropy(logits, labels)\n",
    "\n",
    "        return SequenceClassifierOutput(loss=loss, logits=logits)\n",
    "\n",
    "def get_optimizer_params(model, base_lr, weight_decay, lllrd=0.9):\n",
    "    no_decay = [\"bias\", \"LayerNorm.weight\", \"layer_norm.weight\", \"ln_f.weight\"]\n",
    "    params = []\n",
    "\n",
    "    bb = model.backbone\n",
    "    # common layouts\n",
    "    if hasattr(bb, \"encoder\") and hasattr(bb.encoder, \"layer\"):\n",
    "        layers = list(bb.encoder.layer)\n",
    "        embeddings = getattr(bb, \"embeddings\", None)\n",
    "    elif hasattr(bb, \"deberta\") and hasattr(bb.deberta, \"encoder\"):\n",
    "        layers = list(bb.deberta.encoder.layer)\n",
    "        embeddings = getattr(bb, \"embeddings\", None)\n",
    "    else:\n",
    "        layers = []\n",
    "        embeddings = getattr(bb, \"embeddings\", None)\n",
    "\n",
    "    all_layers = [embeddings] + layers + [model.classifier]\n",
    "    all_layers = [x for x in all_layers if x is not None]\n",
    "    n = len(all_layers)\n",
    "\n",
    "    if n == 0:\n",
    "        return [{\"params\": [p for p in model.parameters() if p.requires_grad],\n",
    "                 \"lr\": base_lr, \"weight_decay\": weight_decay}]\n",
    "\n",
    "    for i, layer in enumerate(all_layers):\n",
    "        lr = base_lr * (lllrd ** (n - i - 1))\n",
    "        named = list(layer.named_parameters())\n",
    "        if not named:\n",
    "            continue\n",
    "        decay = [p for n_, p in named if not any(nd in n_ for nd in no_decay)]\n",
    "        no_dec = [p for n_, p in named if any(nd in n_ for nd in no_decay)]\n",
    "        if decay:\n",
    "            params.append({\"params\": decay, \"lr\": lr, \"weight_decay\": weight_decay})\n",
    "        if no_dec:\n",
    "            params.append({\"params\": no_dec, \"lr\": lr, \"weight_decay\": 0.0})\n",
    "    return params\n"
   ]
